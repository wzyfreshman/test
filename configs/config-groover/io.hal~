#ghf-2014/5/28
# <<section 2>> ------ axis zero index pulse
# connect index pulses to motion controller
# do these when index pulsing is figured out

net Xindex <= ethercat.mt.00.start_probe axis.0.0.index-enable
net Yindex <= ethercat.mt.02.start_probe axis.0.1.index-enable
net Zindex <= ethercat.mt.04.start_probe axis.0.2.index-enable
#net Uindex <= ethercat.mt.01.start_probe axis.1.0.index-enable
#net Vindex <= ethercat.mt.03.start_probe axis.1.1.index-enable
#net Windex <= ethercat.mt.05.start_probe axis.1.2.index-enable

# <<section 8>> ------ the Spindle Value
newsig spindle_value float
net spindle_value motion.0.spindle-da-out ethercat.master.DAVol.0
setp ethercat.master.DAScale.0 [TRAJ]SPINDLEVEL_SCALE

#newsig spindle_value1 float
#net spindle_value1 motion.1.spindle-speed-out ethercat.master.DAVol.1
#setp ethercat.master.DAScale.1 [TRAJ]SPINDLEVEL_SCALE


# <<section 9>> ------ the jog-wheel(handle)
#enable jog-wheel limit velocity
setp axis.0.0.jog-vel-mode 1
setp axis.0.1.jog-vel-mode 1
setp axis.0.2.jog-vel-mode 1
#setp axis.1.0.jog-vel-mode 1
#setp axis.1.1.jog-vel-mode 1
#setp axis.1.2.jog-vel-mode 1



# handle
#net jog_enable axis.0.0.jog-enable axis.1.0.jog-enable  
#net jog_enable axis.0.1.jog-enable axis.1.1.jog-enable  
#net jog_enable axis.0.2.jog-enable axis.1.2.jog-enable  
net jog_enable axis.0.0.jog-enable   
net jog_enable axis.0.1.jog-enable   
net jog_enable axis.0.2.jog-enable 


net handle_data ethercat.master.handleVal handlecontrol.0.ppmcin0
net handle_count00 handlecontrol.0.handleout0 motion.0.jogVal
#net handle_count01 handlecontrol.0.handleout1 motion.1.jogVal



#hong-2014-08-29
#net fpgastatus motion.0.fpgastatus ppmc.0.fpgastatus
setp motion.0.fpgastatus 0xff
#setp motion.1.fpgastatus 0xff
#net ethready motion.0.linktoEth motion.1.linktoEth ethercat.master.ready
net ethready motion.0.linktoEth ethercat.master.ready

#################input#########################
setp conv-io.1.io-type 0

########急停###############
setp conv-io.0.inpin-0 [IO_INPUT0]PIN
setp conv-io.0.inpol-0 [IO_INPUT0]POLARITY
net sig_ioin0 conv-io.0.outpin-0 => not.0.in
#net sig_ioin0 iocontrol.0.0.emc-enable-in iocontrol.1.0.emc-enable-in 
net sig_ioin0 iocontrol.0.0.emc-enable-in 
net not0 not.0.out motion.0.user-define-in.00


########左启动###############
setp conv-io.0.inpin-1 [IO_INPUT1]PIN
setp conv-io.0.inpol-1 [IO_INPUT1]POLARITY
net sig_ioin1 conv-io.0.outpin-1 => motion.0.user-define-in.01
net sig_ioin1 motion.0.digital-in-01

########右启动###############
setp conv-io.0.inpin-2 [IO_INPUT2]PIN
setp conv-io.0.inpol-2 [IO_INPUT2]POLARITY
net sig_ioin2 conv-io.0.outpin-2 => motion.0.user-define-in.02
net sig_ioin2 motion.0.digital-in-02

########左压料到位###############
